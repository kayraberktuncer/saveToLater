<h2 class="page-header">User</h2>
<form action="/users" method="GET">
    <label>Name</label>
    <input type="text" name="name" value="<%= searchOptions.name %>">
    <button type="submit">Search</button>
</form>

<% users.forEach(user=> { %>
    <div>
        <%= user.name %>
    </div>
    <div class="btn-row">
        <a class="btn btn-primary" href="/users/<%= user.id %>">View</a>
        <a class="btn btn-primary" href="/users/<%= user.id %>/edit">Edit</a>
        <%- include('../partials/deleteForm', { url: `/users/${user.id}` }) %>
    </div>
    <% }) %>